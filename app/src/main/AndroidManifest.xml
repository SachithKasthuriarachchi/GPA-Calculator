<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sachith.gpacalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_1_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".impl.DisplayModuleActivityDept" />
        <activity android:name=".impl.DisplayModuleActivityFreshers" />
        <activity android:name=".DisplayCalculatorActivity" />
        <activity android:name=".ShowResult" />
        <activity android:name=".DisplayDeptActivity" />
        <activity android:name=".DisplaySemesters" />
        <activity android:name=".DashBoardActivity" />
    </application>

</manifest>